{
	"editor.tabSize": 2,
  "editor.wrappingIndent": "indent",
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "files.trimTrailingWhitespace": true,

  "files.exclude": {
		"node_modules": true,
    "dist/": true,
		"lib/**/*.js": true,
		"lib/**/*.map": true,
		"lib/**/*.d.ts": true,
    "lib5/**/*.js": true,
		"lib5/**/*.map": true,
		"lib5/**/*.d.ts": true,
		"typings": true,
		"**/__pycache__": true,
		".idea": true
	},

  "typescript.tsdk": "node_modules/typescript/lib",
	"tslint.enable": true
}
